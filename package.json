{
  "name": "v-fullcalendar",
  "version": "0.1.0",
  "description": "The fullcalendar v4 project for vue.",
  "main": "dist/v-fullcalendar.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "demo": "vue-cli-service build",
    "build": "cross-env NODE_ENV=production rollup -c",
    "lint": "vue-cli-service lint",
    "pub": "sh ./publish.sh"
  },
  "keywords": [
    "v-fullcalendar",
    "fullcalendar",
    "Vue.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/taoche/v-fullcalendar"
  },
  "author": "taoche (taoche2010@gmail.com)",
  "peerDependencies": {
    "@fullcalendar/core": "^4.0.1",
    "vue": "^2.2.6"
  },
  "devDependencies": {
    "@fullcalendar/core": "^4.0.1",
    "@fullcalendar/daygrid": "^4.0.1",
    "@fullcalendar/interaction": "^4.0.1",
    "@fullcalendar/list": "^4.0.1",
    "@fullcalendar/timegrid": "^4.0.1",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "raw-loader": "^2.0.0",
    "rollup": "^1.7.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
